#include "main.h"
#include <stdio.h>
/**
 * _strncpy - copies a string
 * @dest: char string tpue
 * @src: char type
 * @n: int type
 */
char *_strncpy(char *dest, char *src, int n)
{
	int i;

	for (i = 0; i < n && src[i] != '\0'; i++)
		dest[i] = src[i];

	for (; n > i; i++)
		dest[i] = '\0';

	return (dest);
}
